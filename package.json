    "babelify": "^7.2.0",
    "browserify": "~10.2.4",
    "browserify-incremental": "^3.0.1",
    "core-js": "^2.4.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "*",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "superagent": "^3.2.1",
    "truncate": "^2.0.0"
  },
  "devDependencies": {
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "disc": "^1.3.2",
    "eslint": "^2.4.0",
    "eslint-config-standard": "^5.1.0",
