<div class="form-group">
  <%= errors_for(f.object) %>
</div>
<div class="form-group">
  <%= f.label :first_name, 'Given Name' %>
  <%= f.text_field :first_name,
      placeholder: 'Given Name', class: 'form-control'%>
</div>
<div class="form-group">
    <%= f.label :last_name, 'Family Name' %>
    <%= f.text_field :last_name, placeholder:
   'Family Name', class: 'form-control' %>
</div>
<div class="form-group">
    <%= f.label :username %>
    <%= f.text_field :username,
      placeholder: 'Username', class: 'form-control'%>
</div>
<div class="form-group">
    <%= f.label :email %>
    <%= f.email_field :email,
      placeholder: 'Email', class: 'form-control'%>
</div>
<div class="form-group">
  <%= f.label :password %>
  <%= f.password_field :password,
      placeholder: 'password', class: 'form-control'%>
</div>
<div class="form-group">
  <%= f.label :password_confirmation, 'Password Confirmation' %>
  <%= f.password_field :password_confirmation,
      placeholder: 'Password Confirmation', class: 'form-control'%>
</div>

