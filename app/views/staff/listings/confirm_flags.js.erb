if(<%= Flag::STATUS[0] == params[:status] %>){
  $('#<%= @listing.id %>-listing').append('The listing flag status has been updated. It will be removed from public view').find('form').remove();
}
else {
  $('#<%= @listing.id %>-listing').append('The listing flag status has been updated. It has been determined to be safe public viewing').find('form').remove();
   $('#<%= @listing.id %>-listing').find('.flag-count').html('0');
}